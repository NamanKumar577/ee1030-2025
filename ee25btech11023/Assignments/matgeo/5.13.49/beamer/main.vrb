\frametitle{Python Code for Solving}
\begin{lstlisting}[language=Python]

M = sympy.Matrix([
        [coeffs[0], -k,         coeffs[1]],
        [k,         coeffs[2],  coeffs[3]],
        [coeffs[4], coeffs[5],  coeffs[6]]
    ])
det_M = M.det()
print(f"\nDeterminant = {det_M}")


solutions = sympy.solve(det_M, k)

print(f"k can be {solutions}")
\end{lstlisting}
